{
	"name": "sourcebroker/configs",
	"description": "Read configs based on TYPO3_CONTEXT settings.",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Inscript Team",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.4 || ^8.0",
		"typo3/cms-core": "^10 || ^11 || ^12"
	},
	"suggest": {},
	"conflict": {},
	"autoload": {
		"psr-4": {
			"SourceBroker\\Configs\\": "Classes/"
		}
	},
	"config": {
		"vendor-dir": ".build/vendor",
		"bin-dir": ".build/bin",
		"allow-plugins": {
			"typo3/cms-composer-installers": true,
			"typo3/class-alias-loader": true
		}
	},
	"extra": {
		"typo3/cms": {
			"app-dir": ".build",
			"web-dir": ".build/public"
		}
	},
	"require-dev": {
		"typo3/coding-standards": "^0.6.1"
	}
}
