{
	"name": "sourcebroker/config-typo3",
	"description": "Read configs based on TYPO3_CONTEXT settings.",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "SourceBroker Team",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.4 || ~8.0",
		"typo3/cms-core": "^10.0.0 || ^11.0.0"
	},
	"suggest": {},
	"conflict": {},
	"autoload": {
		"psr-4": {
			"SourceBroker\\ConfigTypo3\\": "src/"
		}
	},
	"config": {
		"vendor-dir": ".build/vendor",
		"bin-dir": ".build/bin",
		"allow-plugins": {
			"typo3/cms-composer-installers": true,
			"typo3/class-alias-loader": true
		}
	},
	"extra": {
		"typo3/cms": {
			"app-dir": ".build",
			"web-dir": ".build/public"
		}
	}
}
